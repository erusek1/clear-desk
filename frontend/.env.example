# Environment
VITE_NODE_ENV=development

# API Configuration
VITE_API_URL=http://localhost:3001/api
VITE_API_VERSION=v1

# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_S3_BUCKET=clear-desk-files
VITE_CLOUDFRONT_URL=

# Authentication
VITE_AUTH_STORAGE_KEY=cd_auth_token
VITE_AUTH_REFRESH_KEY=cd_refresh_token
VITE_AUTH_EXPIRY_KEY=cd_auth_expiry

# Feature Flags
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_CAMERA_INTEGRATION=false
VITE_ENABLE_ANALYTICS=true

# App Config
VITE_COMPANY_NAME=Clear-Desk.com
VITE_SUPPORT_EMAIL=support@clear-desk.com
VITE_DEFAULT_PAGINATION_LIMIT=20

# Development Tools
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_DEBUG_TOOLS=true
